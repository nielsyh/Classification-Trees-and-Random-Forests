tree.majorityVote <- function(predictions) {     zeros = 0     ones = 0     for (i in predictions) {         if (i == 1) {             ones = ones + 1         }         else {             zeros = zeros + 1         }     }     if (zeros > ones) return(0)     if (ones > zeros) return(1)     #if they're equal, we must choose one randomly     rand <- sample(1:100, 1)     if (rand <= 50) return(0)     else return(1)     }
getConfusionMatrix(label,z)
tree.majority <- function(node) {     height = nrow(node$x)     classes = node$y     agg = 0.0     for (i in classes) {         for (l in i) {             final <- 0             if (l >= 1) {                 final <- 1             }             agg = agg + final         }     }     total = agg / height     if (total <= 0.5) return(0)     return(1) }
